unted() {\n    // 백엔드에서 문어의 초기 레벨을 받아와 업데이트\n    this.updateOctopusLevel(2); // 예시: 2단계로 설정\n  }\n};","map":{"version":3,"names":["ExpenseContainer","OctopusLevel","OctopusSelection","TabBar","name","components","data","expenseAmount","currentLevel","currentOctopus","image","require","octopuses","id","levelImages","methods","selectOctopus","octopus","navigateToProductRecommendation","$router","push","updateOctopusLevel","newLevel","mounted"],"sources":["/Users/gimeus/Desktop/Crunch-It/Crunch-It/Development/FE/muno-muno/src/pages/Home.vue"],"sourcesContent":["<template>\n  <div class=\"homepage\">\n    <!-- 헤더 -->\n    <div class=\"header\">\n      <img src=\"@/assets/logo.svg\" alt=\"Muno 로고\" class=\"logo\" />\n\n      <router-link to=\"/mypage\" class=\"mypage\">\n        <img\n          src=\"@/assets/mypage.svg\"\n          alt=\"마이페이지 아이콘\"\n          class=\"mypage-icon\"\n        />\n      </router-link>\n    </div>\n\n    <!-- 문어 레벨 섹션 -->\n    <OctopusLevel\n      :currentLevel=\"currentLevel\"\n      :currentOctopus=\"currentOctopus\"\n      @navigate=\"navigateToProductRecommendation\"\n    />\n\n    <!-- 문어 선택 섹션 -->\n    <OctopusSelection :octopuses=\"octopuses\" @select-octopus=\"selectOctopus\" />\n\n    <!-- 지출 금액 섹션 -->\n    <ExpenseContainer :expenseAmount=\"expenseAmount\" />\n\n    <!-- TabBar 컴포넌트 사용 -->\n    <TabBar />\n  </div>\n</template>\n\n<script>\nimport ExpenseContainer from '@/components/ExpenseContainer.vue';\nimport OctopusLevel from '@/components/OctopusLevel.vue';\nimport OctopusSelection from '@/components/OctopusSelection.vue';\nimport TabBar from '@/components/TabBar.vue';\n\nexport default {\n  name: 'HomePage',\n  components: {\n    ExpenseContainer,\n    OctopusLevel,\n    OctopusSelection,\n    TabBar,\n  },\n  data() {\n    return {\n      expenseAmount: 100000,\n      currentLevel: 1, // 문어 초기 단계\n      currentOctopus: {\n        name: '예적금 문어',\n        image: require('@/assets/octopus_a1.svg'), // 예적금 문어 초기 이미지\n      },