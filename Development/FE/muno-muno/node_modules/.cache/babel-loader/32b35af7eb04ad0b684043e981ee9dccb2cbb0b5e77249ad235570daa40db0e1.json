어에 맞는 레벨 이미지와 이름으로 변경\n      this.currentOctopus = {\n        name: octopus.name, // 선택된 문어의 이름으로 변경\n        image: octopus.levelImages[this.currentLevel - 1], // 선택된 문어의 레벨에 맞는 이미지로 변경\n      };\n      this.updateLevelText(); // 문어 선택 시 레벨 텍스트 업데이트\n    },\n    navigateToProductRecommendation() {\n      this.$router.push('/product');\n    },\n    updateOctopusLevel(newLevel) {\n      // 백엔드로부터 새로운 레벨을 받아 문어 업데이트\n      this.currentLevel = newLevel;\n      // 현재 선택된 문어의 레벨에 맞게 이미지 업데이트\n      this.currentOctopus.image =\n        this.currentOctopus.levelImages[this.currentLevel - 1];\n      this.updateLevelText(); // 레벨 변경 시 레벨 텍스트 업데이트\n    },\n    updateLevelText() {\n      // 문어의 현재 레벨과 이름에 맞게 텍스트를 업데이트\n      this.levelText = `Lv. ${this.currentLevel} ${this.currentOctopus.name}`;\n    },\n  },\n  mounted() {\n    // 백엔드에서 문어의 초기 레벨을 받아와 업데이트 (여기선 2단계 예시)\n    this.updateOctopusLevel(2);\n  },\n};\n</script>\n"],"mappings":";AAkCA,OAAOA,gBAAe,MAAO,mCAAmC;AAChE,OAAOC,YAAW,MAAO,+BAA+B;AACxD,OAAOC,gBAAe,MAAO,mCAAmC;AAChE,OAAOC,MAAK,MAAO,yBAAyB;AAE5C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVL,gBAAgB;IAChBC,YAAY;IACZC,gBAAgB;IAChBC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,MAAM;MACrBC,YAAY,EAAE,CAAC;MAAE;MACjBC,cAAc,EAAE;QACdL,IAAI,EAAE,QAAQ;QACdM,KAAK,EAAEC,OAAO,CAAC,yBAAyB;MAC1C,CAAC;MACDC,SAAS,EAAE,cAAc;MAAE;MAC3BC,SAAS,EAAE,CACT;QACEC,EAAE,EAAE,CAAC;QACLV,IAAI,EAAE,QAAQ;QACdW,WAAW,EAAE,CACXJ,OAAO,CAAC,yBAAyB,CAAC,EAClCA,OAAO,CAAC,yBAAyB,CAAC,EAClCA,OAAO,CAAC,yBAAyB,CAAC,CACnC;QACDK,WAAW,EAAEL,OAAO,CAAC,uBAAuB;MAC9C,CAAC,EACD;QACEG,EAAE,EAAE,CAAC;QACLV,IAAI,EAAE,OAAO;QACbW,WAAW,EAAE,CACXJ,OAAO,CAAC,yBAAyB,CAAC,EAClCA,OAAO,CAAC,yBAAyB,CAAC,EAClCA,OAAO,CAAC,yBAAyB,CAAC,CACnC;QACDK,WAAW,EAAEL,OAAO,CAAC,uBAAuB;MAC9C,CAAC,EACD;QACEG,EAAE,EAAE,CAAC;QACLV,IAAI,EAAE,OAAO;QACbW,WAAW,EAAE,CACXJ,OAAO,CAAC,yBAAyB,CAAC,EAClCA,OAAO,CAAC,yBAAyB,CAAC,EAClCA,OAAO,CAAC,yBAAyB,CAAC,CACnC;QACDK,WAAW,EAAEL,OAAO,CAAC,uBAAuB;MAC9C,CAAC,EACD;QACEG,EAAE,EAAE,CAAC;QA