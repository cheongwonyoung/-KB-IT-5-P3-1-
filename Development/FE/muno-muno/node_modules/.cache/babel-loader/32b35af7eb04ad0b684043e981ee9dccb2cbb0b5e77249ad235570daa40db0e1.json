{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ExpenseContainer from '@/components/ExpenseContainer.vue';\nimport OctopusLevel from '@/components/OctopusLevel.vue';\nimport OctopusSelection from '@/components/OctopusSelection.vue';\nimport TabBar from '@/components/TabBar.vue';\nexport default {\n  name: 'HomePage',\n  components: {\n    ExpenseContainer,\n    OctopusLevel,\n    OctopusSelection,\n    TabBar\n  },\n  data() {\n    return {\n      expenseAmount: 100000,\n      currentLevel: 1,\n      // 문어 초기 단계\n      currentOctopus: {\n        name: '예적금 문어',\n        image: require('@/assets/octopus_a1.svg')\n      },\n      levelText: 'Lv. 1 예적금 문어',\n      // 문어 레벨 텍스트\n      octopuses: [{\n        id: 1,\n        name: '예적금 문어',\n        levelImages: [require('@/assets/octopus_a1.svg'), require('@/assets/octopus_a2.svg'), require('@/assets/octopus_a3.svg')],\n        buttonImage: require('@/assets/button_a.svg')\n      }, {\n        id: 2,\n        name: '주식 문어',\n        levelImages: [require('@/assets/octopus_b1.svg'), require('@/assets/octopus_b2.svg'), require('@/assets/octopus_b3.svg')],\n        buttonImage: require('@/assets/button_b.svg')\n      }, {\n        id: 3,\n        name: '펀드 문어',\n        levelImages: [require('@/assets/octopus_c1.svg'), require('@/assets/octopus_c2.svg'), require('@/assets/octopus_c3.svg')],\n        buttonImage: require('@/assets/button_c.svg')\n      }, {\n        id: 4,\n        name: '채권 문어',\n        levelImages: [require('@/assets/octopus_d1.svg'), require('@/assets/octopus_d2.svg'), require('@/assets/octopus_d3.svg')],\n        buttonImage: require('@/assets/button_d.svg')\n      }]\n    };\n  },\n  methods: {\n    selectOctopus(octopus) {\n      // 선택한 문어에 맞는 레벨 이미지와 이름으로 변경\n      this.currentOctopus = {\n        name: octopus.name,\n        // 선택된 문어의 이름으로 변경\n        image: octopus.levelImages[this.currentLevel - 1] // 선택된 문어의 레벨에 맞는 이미지로 변경\n      };\n      this.updateLevelText(); // 문어 선