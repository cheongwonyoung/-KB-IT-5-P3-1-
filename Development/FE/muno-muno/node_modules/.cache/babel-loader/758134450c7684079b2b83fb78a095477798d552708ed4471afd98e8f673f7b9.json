어 레벨 섹션 -->\n    <section class=\"level-section\">\n      <p class=\"level-text\">Lv. {{ currentLevel }} {{ currentOctopus.name }}</p>\n      <div class=\"progress-bar\">\n        <div\n          class=\"progress\"\n          :style=\"{ width: currentLevelProgress + '%' }\"\n        ></div>\n      </div>\n      <img\n        :src=\"currentOctopus.image\"\n        alt=\"문어 이미지\"\n        class=\"octopus-image\"\n      />\n    </section>\n\n    <!-- 문어 선택 버튼 -->\n    <div class=\"octopus-selection\">\n      <button\n        v-for=\"octopus in octopuses\"\n        :key=\"octopus.id\"\n        @click=\"selectOctopus(octopus)\"\n        class=\"octopus-button\"\n      >\n        <img :src=\"octopus.buttonImage\" :alt=\"octopus.name\" />\n      </button>\n    </div>\n\n    <!-- 지출 금액 섹션 -->\n    <section class=\"expense-section\">\n      <div class=\"expense-container\">\n        <div class=\"expense-header\">\n          <p class=\"subtitle\">{{ currentMonth }} 지출금액</p>\n          <router-link to=\"/property\" class=\"view-analysis-link\">\n            전체 자산분석 보기 >\n          </router-link>\n        </div>\n        <h1 class=\"amount\">{{ formatCurrency(expenseAmount) }}</h1>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      currentMonth: '9월',\n      expenseAmount: 100000,\n      currentLevel: 3,\n      currentLevelProgress: 70,\n      currentOctopus: {\n        name: '예적금무노',\n        image: '@/assets/octopus_level_3.png',\n      },\n      octopuses: [\n        {\n          id: 1,\n          name: '저축무노',\n          image: '@/assets/octopus_level_1.png',\n          buttonImage: '@/assets/octopus_button_1.png',\n        },\n        {\n          id: 2,\n          name: '투자무노',\n          image: '@/assets/octopus_level_2.png',\n          buttonImage: '@/assets/octopus_button_2.png',\n        },\n        {\n          id: 3,\n          name: '예적금무노',\n          image: '@/assets/octopus_level_3.png',\n          buttonImage: '@/assets/octopus_button_3.png',\n        },\n        {\n          id: 4,\n          name: '자산관리무노',\n          image: '@/assets/octopus_level_4.png',\n          buttonImage: '@/assets/octopus_button_4.png',\n        },\n      ],\n    };\n  },\n  methods: {\n    selectOctopus(octopus) {\n      this.currentOctopus = octopus;\n    },\n    formatCurrency(value) {\n      return new Intl.NumberFormat('ko-KR', {\n        style: 'currency',\n        currency: 'KRW',\n      }).format(value);\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* 전체 페이지 스크롤 숨기기 */\n.homepage {\n  background-color: var(--p70);\n  overflow-y: hidden;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  height: 100vh;\n}\n\n/* 헤더 */\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 22px;\n  height: 65px;\n  background-color: var(--p70);\n  position: fixed;\n  top: 44px;\n  left: 0;\n  width: 100%;\n  z-index: 1000;\n  box-sizing: border-box;\n}\n\n.mypage-icon {\n  width: 24px;\n  height: 24px;\n}\n\n/* 문어 레벨 섹션 */\n.level-section {\n  text-align: center;\n  margin-top: 80px;\n}\n\n.level-text {\n  font-size: 16px;\n  font-weight: 600;\n  color: #4a4a4a;\n}\n\n.progress {\n  background-color: var(--p10);\n  height: 100%;\n  border-radius: 5px;\n}\n\n.octopus-image {\n  width: 220px;\n  height: 220px;\n  margin: 20px auto;\n  object-fit: contain;\n}\n\n/* 문어 선택 버튼 */\n.octopus-selection {\n  display: flex;\n  justify-content: center;\n  gap: 10px;\n  margin-bottom: 20px;\n}\n\n.octopus-selection button {\n  background-color: transparent;\n  border: none;\n  cursor: pointer;\n}\n\n.octopus-selection img {\n  width: 50px;\n  height: 50px;\n}\n\n/* 지출 금액 섹션 */\n.expense-section {\n  position: fixed;\n  bottom: 86px;\n  left: 0;\n  width: 100%;\n  padding: 0;\n}\n\n.expense-container {\n  background-color: var(--p10);\n  border-radius: 20px 20px 0 0;\n  padding: 20px;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.expense-header {\n  display: flex;\n  justify-content: space-between;\n  width: 100%;\n}\n\n.subtitle {\n  color: var(--gr100);\n  font-size: 18px;\n  font-weight: 400;\n  line-height: 100%;\n}\n\n.view-analysis-link {\n  font-size: 14px;\n  color: var(--gr90);\n  text-decoration: none;\n}\n\n.amount {\n  margin-top: 10px;\n  color: var(--gr100);\n  font-size: 32px;\n  font-weight: 400;\n  line-height: 100%;\n}\n</style>\n"],"mappings":"AA2DA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,IAAI;MAClBC,aAAa,EAAE,MAAM;MACrBC,YAAY,EAAE,CAAC;MACfC,oBAAoB,EAAE,EAAE;MACxBC,cAAc,EAAE;QACdN,IAAI,EAAE,OAAO;QACbO,KAAK,EAAE;MACT,CAAC;MACDC,SAAS,EAAE,CACT;QACEC,EAAE,EAAE,CAAC;QACLT,IAAI,EAAE,MAAM;QACZO,KAAK,EAAE,8BAA8B;QACrCG,WAAW,EAAE;MACf,CAAC,EACD;QACED,EAAE,EAAE,CAAC;QACLT,IAAI,EAAE,MAAM;QACZO,KAAK,EAAE,8BAA8B;QACrCG,WAAW,EAAE;MACf,CAAC,EACD;QACED,EAAE,EAAE,CAAC;QACLT,IAAI,EAAE,OAAO;QACbO,KAAK,EAAE,8BAA8B;QACrCG,WAAW,EAAE;MACf,CAAC,EACD;QACED,EAAE,EAAE,CAAC;QACLT,IAAI,EAAE,QAAQ;QACdO,KAAK,EAAE,8BAA8B;QACrCG,WAAW,EAAE;MACf,CAAC;IAEL,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAACC,OAAO,EAAE;MACrB,IAAI,CAACP,cAAa,GAAIO,OAAO;IAC/B,CAAC;IACDC,cAAcA,CAACC,KAAK,EAAE;MACpB,OAAO,IAAIC,IAAI,CAACC,YAAY,CAAC,OAAO,EAAE;QACpCC,KAAK,EAAE,UAAU;QACjBC,QAAQ,EAAE;MACZ,CAAC,CAAC,CAACC,MAAM,CAACL,KAAK,CAAC;IAClB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}