 class=\"octopus-selection\">\n      <button\n        v-for=\"octopus in octopuses\"\n        :key=\"octopus.id\"\n        @click=\"selectOctopus(octopus)\"\n        class=\"octopus-button\"\n      >\n        <div class=\"octopus-box\">\n          <div class=\"octopus-icon-box\"></div>\n          <p class=\"octopus-text\">{{ octopus.name }}</p>\n        </div>\n      </button>\n    </div>\n\n    <!-- 지출 금액 섹션 -->\n    <section class=\"expense-section\">\n      <div class=\"expense-container\">\n        <!-- 네모 박스 -->\n        <div class=\"expense-box\">\n          <p class=\"subtitle\">지출금액</p>\n          <h1 class=\"amount\">{{ formattedExpenseAmount }}원</h1>\n        </div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      currentMonth: '9월',\n      expenseAmount: 100000,\n      currentLevel: 3,\n      currentOctopus: {\n        name: '예적금 문어',\n      },\n      octopuses: [\n        { id: 1, name: '예적금' },\n        { id: 2, name: '주식' },\n        { id: 3, name: '펀드' },\n        { id: 4, name: '채권' },\n      ],\n    };\n  },\n  computed: {\n    formattedExpenseAmount() {\n      return this.expenseAmount.toLocaleString(); // 숫자를 100,000 형식으로 변환\n    },\n  },\n  methods: {\n    selectOctopus(octopus) {\n      this.currentOctopus = octopus;\n    },\n    navigateToProductRecommendation() {\n      this.$router.push('/product'); // 상품 추천 페이지로 이동\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* 전체 페이지 스크롤 숨기기 */\n.homepage {\n  background-color: var(--p70);\n  overflow-y: hidden;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  height: 100vh;\n}\n\n/* 헤더 */\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 22px;\n  height: 65px;\n  background-color: var(--p70);\n  position: fixed;\n  top: 44px;\n  left: 0;\n  width: 100%;\n  z-index: 1000;\n  box-sizing: border-box;\n}\n\n.mypage-icon {\n  width: 24px;\n  height: 24px;\n}\n\n/* 문어 레벨 섹션 */\n.level-section {\n  margin: 0 22px;\n  margin-top: 91px;\n}\n\n.level-text {\n  color: var(--gray-20);\n  font-size: 24px;\n  font-weight: 500;\n  line-height: 100%;\n  margin: 0 0 10px 0;\n}\n\n.level-description {\n  color: var(--gr50);\n  font-size: 14px;\n  font-weight: 400;\n  line-height: 100%;\n  margin: 10px 0 0 0;\n  cursor: pointer;\n}\n\n/* 이미지가 없을 때 기본 상자 */\n.octopus-image-placeholder {\n  width: 220px;\n  height: 220px;\n  background-color: lightgray; /* 이미지가 없을 때 배경색 */\n  margin: 20px auto;\n  border-radius: 12px;\n}\n\n/* 문어 선택 버튼 */\n.octopus-selection {\n  display: flex;\n  justify-content: center;\n  gap: 14px;\n  margin-bottom: 20px;\n}\n\n.octopus-button {\n  background-color: transparent;\n  border: none;\n  cursor: pointer;\n}\n\n.octopus-box {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  width: 38px; /* 네모 박스 크기 */\n  height: 38px;\n}\n\n.octopus-icon-box {\n  width: 38px;\n  height: 38px;\n  background-color: lightgray; /* 박스 배경색 */\n  border-radius: 8px;\n  margin-bottom: 5px;\n}\n\n.octopus-text {\n  font-size: 12px;\n  text-align: center;\n  color: var(--gr50);\n}\n\n/* 지출 금액 섹션 */\n.expense-section {\n  position: fixed;\n  bottom: 86px;\n  left: 0;\n  width: 100%;\n  padding: 0;\n}\n\n.expense-container {\n  background-color: var(--p10);\n  border-radius: 20px 20px 0 0;\n  padding: 20px 0; /* 상하 패딩 조정 */\n  flex-direction: column;\n  align-items: center;\n}\n\n/* 지출 금액 네모 박스 */\n.expense-box {\n  width: calc(100% - 40px);\n  max-width: 335px;\n  height: 64px;\n  flex-shrink: 0;\n  border-radius: 12px;\n  border: 0.816px solid rgba(255, 255, 255, 0.1);\n  background: rgba(255, 255, 255, 0.2);\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  padding: 0 24px;\n  box-sizing: border-box;\n  margin: 0 20px;\n}\n\n/* 지출 금액 및 금액 표시 */\n.subtitle {\n  color: var(--gr100);\n  font-size: 16px;\n  font-style: normal;\n  font-weight: 400;\n  line-height: 100%;\n}\n\n.amount {\n  color: var(--gr100);\n  text-align: right;\n  font-size: 24px;\n  font-weight: 400;\n  line-height: 100%;\n}\n</style>\n"],"mappings":";AAsDA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,IAAI;MAClBC,aAAa,EAAE,MAAM;MACrBC,YAAY,EAAE,CAAC;MACfC,cAAc,EAAE;QACdL,IAAI,EAAE;MACR,CAAC;MACDM,SAAS,EAAE,CACT;QAAEC,EAAE,EAAE,CAAC;QAAEP,IAAI,EAAE;MAAM,CAAC,EACtB;QAAEO,EAAE,EAAE,CAAC;QAAEP,IAAI,EAAE;MAAK,CAAC,EACrB;QAAEO,EAAE,EAAE,CAAC;QAAEP,IAAI,EAAE;MAAK,CAAC,EACrB;QAAEO,EAAE,EAAE,CAAC;QAAEP,IAAI,EAAE;MAAK,CAAC;IAEzB,CAAC;EACH,CAAC;EACDQ,QAAQ,EAAE;IACRC,sBAAsBA,CAAA,EAAG;MACvB,OAAO,IAAI,CAACN,aAAa,CAACO,cAAc,CAAC,CAAC,EAAE;IAC9C;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAACC,OAAO,EAAE;MACrB,IAAI,CAACR,cAAa,GAAIQ,OAAO;IAC/B,CAAC;IACDC,+BAA+BA,CAAA,EAAG;MAChC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,UAAU,CAAC,EAAE;IACjC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}