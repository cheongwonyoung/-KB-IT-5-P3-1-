h('/product');\n    },\n    updateOctopusLevel(newLevel) {\n      // 백엔드로부터 새로운 레벨을 받아 문어 레벨 업데이트\n      this.currentLevel = newLevel;\n      if (this.currentOctopus) {\n        this.currentOctopus.image = this.currentOctopus.levelImages[this.currentLevel - 1];\n        this.levelText = `Lv.${this.currentLevel} ${this.currentOctopus.name}`;\n      }\n    },\n    fetchLevelFromBackend() {\n      // 백엔드에서 레벨을 받아오는 함수 (API 호출 예시)\n      // 이 예시는 가상의 백엔드 API 호출을 가정합니다.\n      fetch('/api/octopus/level') // 여기에 실제 백엔드 API 경로를 넣으세요\n      .then(response => response.json()).then(data => {\n        this.updateOctopusLevel(data.level); // 받아온 레벨 데이터로 문어 레벨 업데이트\n      }).catch(error => {\n        console.error('Error fetching level from backend:', error);\n      });\n    },\n    setDefaultOctopus() {\n      // 기본적으로 예적금 문어를 선택\n      const defaultOctopus = this.octopuses.find(octopus => octopus.id === 1);\n      this.selectOctopus(defaultOctopus);\n    }\n  },\n  mounted() {\n    this.fetchLevelFromBackend(); // 백엔드에서 초기 레벨 가져오기\n    this.setDefaultOctopus(); // 기본적으로 예적금 문어 설정\n  }\n};","map":{"version":3,"names":["ExpenseContainer","OctopusLevel","OctopusSelection","TabBar","name","components","data","expenseAmount","currentLevel","levelText","currentOctopus","octopuses","id","levelImages","require","buttonImage","methods","selectOctopus","octopus","image","navigateToProductRecommendation","$router","push","updateOctopusLevel","newLevel","fetchLevelFromBackend","fetch","then","response","json","level","catch","error","console","setDefaultOctopus","defaultOctopus","find","mounted"],"sources":["/Users/gimeus/Desktop/Crunch-It/Crunch-It/Development/FE/muno-muno/src/pages/Home.vue"],"sourcesContent":["<template>\n  <div class=\"homepage\">\n    <!-- 헤더 -->\n    <div class=\"header\">\n      <img src=\"@/assets/munoLogo.svg\" alt=\"Muno 로고\" class=\"logo\" />\n\n      <router-link to=\"/mypage\" class=\"mypage\">\n        <img\n          src=\"@/assets/mypage.svg\"\n          alt=\"마이페이지 아이콘\"\n          class=\"mypage-icon\"\n        />\n      </router-link>\n    </div>\n\n    <!-- 문어 레벨 섹션 -->\n    <OctopusLevel\n      :currentLevel=\"currentLevel\"\n      :currentOctopus=\"currentOctopus\"\n      :levelText=\"levelText\"\n      @navigate=\"navigateToProductRecommendation\"\n    />\n\n    <!-- 문어 선택 섹션 -->\n    <OctopusSelection :octopuses=\"octopuses\" @select-octopus=\"selectOctopus\" />\n\n    <!-- 지출 금액 섹션 -->\n    <ExpenseContainer :expenseAmount=\"expenseAmount\" />\n\n    <!-- TabBar 컴포넌트 사용 -->\n    <TabBar />\n  </div>\n</template>\n\n<script>\nimport ExpenseContainer from '@/components/ExpenseContainer.vue';\nimport OctopusLevel from '@/components/OctopusLevel.vue';\nimport OctopusSelection from '@/components/OctopusSelection.vue';\nimport TabBar from '@/components/TabBar.vue';\n\nexport default {\n  name: 'HomePage',\n  components: {\n    ExpenseContainer,\n    OctopusLevel,\n    OctopusSelection,\n    TabBar,\n  },\n  data() {\n    return {\n      expenseAmount: 100000,\n      currentLevel: 1, // 기본 레벨은 1단계로 설정\n      levelText: '', // 초기에는 아무것도 표시되지 않음\n      currentOctopus: null, // 초기에는 선택된 문어가 없음\n      octopuses: [\n        {\n          id: 1,\n          name: '예적금',\n          levelImages: [\n            require('@/assets/octopus_a1.svg'),\n            require('@/assets/octopus_a2.svg'),\n            require('@/assets/octopus_a3.svg'),\n          ],\n          buttonImage: require('@/assets/button_a.svg'),\n        },\n        {\n          id: 2,\n          name: '주식',\n          levelImages: [\n            require('@/assets/octopus_b1.svg'),\n            require('@/assets/octopus_b2.svg'),\n            require('@/assets/octopus_b3.svg'),\n          ],\n          buttonImage: require('@/assets/button_b.svg'),\n        },\n        {\n          id: 3,\n          name: '펀드',\n          levelImages: [\n            require('@/assets/octopus_c1.svg'),\n            require('@/assets/octopus_c2.svg'),\n            require('@/assets/octopus_c3.svg'),\n          ],\n          buttonImage: require('@/assets/button_c.svg'),\n        },\n        {\n          id: 4,\n          name: '채권',\n          levelImages: [\n            require('@/assets/octopus_d1.svg'),\n            require('@/assets/octopus_d2.svg'),\n            require('@/assets/octopus_d3.svg'),\n          ],\n          buttonImage: require('@/assets/button_d.svg'),\n        },\n      ],\n    };\n  },\n  methods: {\n    selectOctopus(octopus) {\n      // 선택한 문어와 현재 레벨에 맞는 이미지를 적용\n      this.currentOctopus = {\n        name: octopus.name,\n        image: octopus.levelImages[this.currentLevel - 1], // 현재 레벨에 맞는 이미지 설정\n      };\n      this.levelText = `Lv.${this.currentLevel} ${octopus.name}`;\n    },\n    navigateToProductRecommendation() {\n      this.$router.push('/product');\n    },\n    updateOctopusLevel(newLevel) {\n      // 백엔드로부터 새로운 레벨을 받아 문어 레벨 업데이트\n      this.currentLevel = newLevel;\n      if (this.currentOctopus) {\n        this.currentOctopus.image =\n          this.currentOctopus.levelImages[this.currentLevel - 1];\n        this.levelText = `Lv.${this.currentLevel} ${this.currentOctopus.name}`;\n      }\n    },\n    fetchLevelFromBackend() {\n      // 백엔드에서 레벨을 받아오는 함수 (API 호출 예시)\n      // 이 예시는 가상의 백엔드 API 호출을 가정합니다.\n      fetch('/api/octopus/level') // 여기에 실제 백엔드 API 경로를 넣으세요\n        .then((response) => response.json())\n        .then((data) => {\n          this.updateOctopusLevel(data.level); // 받아온 레벨 데이터로 문어 레벨 업데이트\n        })\n        .catch((error) => {\n          console.error('Error fetching level from backend:', error);\n        });\n    },\n    setDefaultOctopus() {\n      // 기본적으로 예적금 문어를 선택\n      const defaultOctopus = this.octopuses.find((octopus) => octopus.id === 1);\n      this.selectOctopus(defaultOctopus);\n    },\n  },\n  mounted() {\n    this.fetchLevelFromBackend(); // 백엔드에서 초기 레벨 가져오기\n    this.setDefaultOctopus(); // 기본적으로 예적금 문어 설정\n  },\n};\n</script>\n\n<style scoped>\n.homepage {\n 