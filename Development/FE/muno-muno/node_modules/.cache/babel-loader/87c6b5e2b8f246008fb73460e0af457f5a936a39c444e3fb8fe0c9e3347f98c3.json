t>\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      currentMonth: '9월',\n      expenseAmount: 100000,\n      currentLevel: 3,\n      currentOctopus: {\n        name: '예적금 문어',\n        image: '@/assets/octopus_level_3.png',\n      },\n      octopuses: [\n        {\n          id: 1,\n          name: '예적금',\n          buttonImage: '@/assets/octopus_button_1.png',\n        },\n        {\n          id: 2,\n          name: '주식',\n          buttonImage: '@/assets/octopus_button_2.png',\n        },\n        {\n          id: 3,\n          name: '펀드',\n          buttonImage: '@/assets/octopus_button_3.png',\n        },\n        {\n          id: 4,\n          name: '채권',\n          buttonImage: '@/assets/octopus_button_4.png',\n        },\n      ],\n    };\n  },\n  computed: {\n    formattedExpenseAmount() {\n      return this.expenseAmount.toLocaleString(); // 숫자를 100,000 형식으로 변환\n    },\n  },\n  methods: {\n    selectOctopus(octopus) {\n      this.currentOctopus = octopus;\n    },\n    navigateToProductRecommendation() {\n      this.$router.push('/product'); // 상품 추천 페이지로 이동\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* 전체 페이지 스크롤 숨기기 */\n.homepage {\n  background-color: var(--p70);\n  overflow-y: hidden;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  height: 100vh;\n}\n\n/* 헤더 */\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 22px;\n  height: 65px;\n  background-color: var(--p70);\n  position: fixed;\n  top: 44px;\n  left: 0;\n  width: 100%;\n  z-index: 1000;\n  box-sizing: border-box;\n}\n\n.mypage-icon {\n  width: 24px;\n  height: 24px;\n}\n\n/* 문어 레벨 섹션 */\n.level-section {\n  margin: 0 22px;\n  margin-top: 91px;\n}\n\n.level-text {\n  color: var(--gray-20);\n  font-size: 24px;\n  font-weight: 500;\n  line-height: 100%;\n  margin: 0 0 10px 0;\n}\n\n.level-description {\n  color: var(--gr50);\n  font-size: 14px;\n  font-weight: 400;\n  line-height: 100%;\n  margin: 10px 0 0 0;\n  cursor: pointer;\n}\n\n.octopus-image {\n  width: 220px;\n  height: 220px;\n  margin: 20px auto;\n  object-fit: contain;\n}\n\n/* 아이콘 버튼 섹션 */\n.octopus-selection {\n  display: flex;\n  justify-content: center;\n  gap: 14px;\n  margin-bottom: 20px;\n}\n\n.octopus-button {\n  background-color: transparent;\n  border: none;\n  cursor: pointer;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.octopus-icon {\n  width: 38px;\n  height: 38px;\n}\n\n.octopus-text {\n  font-size: 12px;\n  margin-top: 5px;\n  text-align: center;\n  color: var(--gr50);\n}\n\n/* 지출 금액 섹션 */\n.expense-section {\n  position: fixed;\n  bottom: 86px;\n  left: 0;\n  width: 100%;\n  padding: 0;\n}\n\n.expense-container {\n  background-color: var(--p10);\n  border-radius: 20px 20px 0 0;\n  padding: 20px 0; /* 상하 패딩 조정 */\n  flex-direction: column;\n  align-items: center;\n}\n\n/* 지출 금액 네모 박스 */\n.expense-box {\n  width: calc(100% - 40px);\n  max-width: 335px;\n  height: 64px;\n  flex-shrink: 0;\n  border-radius: 12px;\n  border: 0.816px solid rgba(255, 255, 255, 0.1);\n  background: rgba(255, 255, 255, 0.2);\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  padding: 0 24px;\n  box-sizing: border-box;\n  margin: 0 20px;\n}\n\n/* 지출 금액 및 금액 표시 */\n.subtitle {\n  color: var(--gr100);\n  font-size: 16px;\n  font-style: normal;\n  font-weight: 400;\n  line-height: 100%;\n}\n\n.amount {\n  color: var(--gr100);\n  text-align: right;\n  font-size: 24px;\n  font-weight: 400;\n  line-height: 100%;\n}\n</style>\n"],"mappings":";OAIWA,UAA2B;OAI5BC,UAAyB;;EAP5BC,KAAK,EAAC;AAAU;;EAEdA,KAAK,EAAC;AAAQ;;EAaVA,KAAK,EAAC;AAAe;;EACzBA,KAAK,EAAC;AAAY;mBAjB3B;;EA6BSA,KAAK,EAAC;AAAmB;mBA7BlC;mBAAA;;EAyCWA,KAAK,EAAC;AAAc;;EAKlBA,KAAK,EAAC;AAAiB;;EACzBA,KAAK,EAAC;AAAmB;;EAEvBA,KAAK,EAAC;AAAa;;EAElBA,KAAK,EAAC;AAAQ;;;uBAlD1BC,mBAAA,CAsDM,OAtDNC,UAsDM,GArDJC,mBAAA,QAAW,EACXC,mBAAA,CAUM,OAVNC,UAUM,G,0BATJD,mBAAA,CAA8D;IAAzDE,GAA2B,EAA3BR,UAA2B;I