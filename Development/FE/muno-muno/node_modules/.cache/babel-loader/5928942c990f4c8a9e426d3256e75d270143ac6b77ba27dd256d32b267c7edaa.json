ustify-content: space-between;\n  align-items: center;\n  position: fixed;\n  bottom: 0;\n  left: 0;\n  width: 100%;\n  height: 85px;\n  background-color: var(--gr100);\n  border-top: 1px solid var(--gr70);\n  box-shadow: 0px -1px 5px rgba(0, 0, 0, 0.1);\n  z-index: 1000;\n}\n\n.tab-item {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  text-align: center;\n  color: var(--gr60);\n  text-decoration: none;\n  width: calc(100% / 4);\n  padding: 8px 0;\n  padding-bottom: 34px;\n}\n\n.tab-item img {\n  width: 24px;\n  height: 24px;\n  margin-bottom: 2.4px; /* img와 span 사이 간격 설정 */\n}\n\n.tab-item span {\n  font-size: 12px;\n  font-weight: 500;\n  text-align: center;\n  margin-top: 0px; /* span의 상단 여백을 줄임 */\n}\n\n.tab-item.active {\n  color: var(--p10);\n}\n\n.tab-item.active span {\n  color: var(--p10);\n}\n\n.bar {\n  position: absolute;\n  bottom: 0;\n  left: 0;\n  width: 100%;\n  height: 34px;\n  z-index: -1;\n  object-fit: contain;\n}\n</style>\n"],"mappings":"AAuEA,eAAe;EACbA,IAAI,EAAE;AACR,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}