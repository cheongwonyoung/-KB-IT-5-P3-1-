{\n      // 백엔드로부터 새로운 레벨을 받아 문어를 업데이트\n      this.currentLevel = newLevel;\n      // 현재 선택된 문어의 레벨에 맞게 이미지 변경\n      this.currentOctopus.image =\n        this.currentOctopus.levelImages[this.currentLevel - 1];\n    },\n  },\n  mounted() {\n    // 백엔드에서 문어의 초기 레벨을 받아와 업데이트\n    this.updateOctopusLevel(2); // 예시: 2단계로 설정\n  },\n};\n</script>\n\n<style scoped>\n.homepage {\n  background-color: var(--p70);\n  overflow-y: hidden;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  height: 100vh;\n}\n\n/* 기존 헤더 스타일 유지 */\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 22px;\n  height: 65px;\n  background-color: var(--p70);\n  position: fixed;\n  top: 44px;\n  left: 0;\n  width: 100%;\n  z-index: 1000;\n  box-sizing: border-box;\n}\n\n.mypage-icon {\n  width: 24px;\n  height: 24px;\n}\n</style>\n"],"mappings":";OAIWA,UAA2B;OAI5BC,UAAyB;;EAP5BC,KAAK,EAAC;AAAU;;EAEdA,KAAK,EAAC;AAAQ;;;;;;;uBAFrBC,mBAAA,CA6BM,OA7BNC,UA6BM,GA5BJC,mBAAA,QAAW,EACXC,mBAAA,CAUM,OAVNC,UAUM,G,0BATJD,mBAAA,CAA8D;IAAzDE,GAA2B,EAA3BR,UAA2B;IAACS,GAAG,EAAC,SAAS;IAACP,KAAK,EAAC;+BAErDQ,YAAA,CAMcC,sBAAA;IANDC,EAAE,EAAC,SAAS;IAACV,KAAK,EAAC;;IANtCW,OAAA,EAAAC,QAAA,CAOQ,MAIEC,MAAA,QAAAA,MAAA,OAJFT,mBAAA,CAIE;MAHAE,GAAyB,EAAzBP,UAAyB;MACzBQ,GAAG,EAAC,WAAW;MACfP,KAAK,EAAC;;IAVhBc,CAAA;QAeIX,mBAAA,cAAiB,EACjBK,YAAA,CAIEO,uBAAA;IAHCC,YAAY,EAAEC,KAAA,CAAAD,YAAY;IAC1BE,cAAc,EAAED,KAAA,CAAAC,cAAc;IAC9BC,UAAQ,EAAEC,QAAA,CAAAC;6EAGblB,mBAAA,cAAiB,EACjBK,YAAA,CAA2Ec,2BAAA;IAAxDC,SAAS,EAAEN,KAAA,CAAAM,SAAS;IAAGC,eAAc,EAAEJ,QAAA,CAAAK;6DAE1DtB,mBAAA,cAAiB,EACjBK,YAAA,CAAmDkB,2BAAA;IAAhCC,aAAa,EAAEV,KAAA,CAAAU;EAAa,4CAE/CxB,mBAAA,oBAAuB,EACvBK,YAAA,CAAUoB,iBAAA,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}