n      };\n      this.levelText = `Lv.${this.currentLevel} ${octopus.name}`;\n    },\n    navigateToProductRecommendation() {\n      this.$router.push('/product');\n    },\n    updateOctopusLevel() {\n      const currentDate = new Date();\n      // 매월 1일이면 레벨을 Lv.1로 초기화\n      if (currentDate.getDate() === 1) {\n        this.currentLevel = 1;\n      } else {\n        this.currentLevel = 3; // 예시로 기본 3단계로 설정\n      }\n      this.currentOctopus.image =\n        this.currentOctopus.levelImages[this.currentLevel - 1];\n      this.levelText = `Lv.${this.currentLevel} ${this.currentOctopus.name}`;\n    },\n  },\n  mounted() {\n    this.updateOctopusLevel();\n  },\n};\n</script>\n\n<style scoped>\n.homepage {\n  background-color: var(--p70);\n  overflow-y: hidden;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  height: 100vh;\n}\n\n/* 헤더 유지 */\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 22px;\n  height: 65px;\n  background-color: var(--p70);\n  position: fixed;\n  top: 44px;\n  left: 0;\n  width: 100%;\n  z-index: 1000;\n  box-sizing: border-box;\n}\n\n.mypage-icon {\n  width: 24px;\n  height: 24px;\n}\n</style>\n"],"mappings":";AAmCA,OAAOA,gBAAe,MAAO,mCAAmC;AAChE,OAAOC,YAAW,MAAO,+BAA+B;AACxD,OAAOC,gBAAe,MAAO,mCAAmC;AAChE,OAAOC,MAAK,MAAO,yBAAyB;AAE5C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVL,gBAAgB;IAChBC,YAAY;IACZC,gBAAgB;IAChBC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,MAAM;MACrBC,YAAY,EAAE,CAAC;MAAE;MACjBC,SAAS,EAAE,aAAa;MACxBC,cAAc,EAAE;QACdN,IAAI,EAAE,QAAQ;QACdO,KAAK,EAAEC,OAAO,CAAC,yBAAyB;MAC1C,CAAC;MACDC,SAAS,EAAE,CACT;QACEC,EAAE,EAAE,CAAC;QACLV,IAAI,EAAE,KAAK;QACXW,WAAW,EAAE,CACXH,OAAO,CAAC,yBAAyB,CAAC,EAClCA,OAAO,CAAC,yBAAyB,CAAC,EAClCA,OAAO,CAAC,yBAAyB,CAAC,CACnC;QACDI,WAAW,EAAEJ,OAAO,CAAC,uBAAuB;MAC9C,CAAC,EACD;QACEE,EAAE,EAAE,CAAC;QACLV,IAAI,EAAE,IAAI;QACVW,WAAW,EAAE,CACXH,OAAO,CAAC,yBAAyB,CAAC,EAClCA,OAAO,CAAC,yBAAyB,CAAC,EAClCA,OAAO,CAAC,yBAAyB,