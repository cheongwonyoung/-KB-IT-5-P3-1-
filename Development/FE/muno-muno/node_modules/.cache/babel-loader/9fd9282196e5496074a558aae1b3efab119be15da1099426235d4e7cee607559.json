n        <img\n          src=\"@/assets/mypage.svg\"\n          alt=\"마이페이지 아이콘\"\n          class=\"mypage-icon\"\n        />\n      </router-link>\n    </div>\n\n    <!-- 문어 레벨 섹션 -->\n    <section class=\"level-section\">\n      <p class=\"level-text\">Lv. {{ currentLevel }} {{ currentOctopus.name }}</p>\n      <router-link to=\"/recommend\" class=\"level-description\">\n        문어를 성장시키고 싶다면? >\n      </router-link>\n      <img\n        :src=\"currentOctopus.image\"\n        alt=\"문어 이미지\"\n        class=\"octopus-image\"\n      />\n    </section>\n\n    <!-- 투자 상품 선택 버튼 (예적금, 주식, 펀드, 채권) -->\n    <div class=\"investment-buttons\">\n      <button\n        v-for=\"investment in investments\"\n        :key=\"investment.id\"\n        class=\"investment-button\"\n      >\n        <img :src=\"investment.icon\" alt=\"상품 아이콘\" />\n        <p>{{ investment.name }}</p>\n      </button>\n    </div>\n\n    <!-- 지출 금액 섹션 -->\n    <section class=\"expense-section\">\n      <div class=\"expense-container\">\n        <!-- 네모 박스 -->\n        <div class=\"expense-box\">\n          <!-- 지출금액 -->\n          <p class=\"subtitle\">지출금액</p>\n          <!-- 금액 -->\n          <h1 class=\"amount\">{{ formattedExpenseAmount }}원</h1>\n        </div>\n      </div>\n    </section>\n\n    <!-- 하단 네비게이션 바 -->\n    <nav class=\"tabbar\">\n      <router-link to=\"/\" class=\"tabbar-item\">홈</router-link>\n      <router-link to=\"/asset\" class=\"tabbar-item\">자산분석</router-link>\n      <router-link to=\"/recommend\" class=\"tabbar-item\">상품추천</router-link>\n      <router-link to=\"/community\" class=\"tabbar-item\">커뮤니티</router-link>\n    </nav>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      currentMonth: '9월',\n      expenseAmount: 100000,\n      currentLevel: 3,\n      currentOctopus: {\n        name: '예적금 문어',\n        image: '@/assets/octopus_level_3.png',\n      },\n      octopuses: [\n        {\n          id: 1,\n          name: '예적금 문어',\n          image: '@/assets/octopus_button_1.png',\n          buttonImage: '@/assets/octopus_button_1.png',\n        },\n        {\n          id: 2,\n          name: '주식 문어',\n          image: '@/assets/octopus_button_2.png',\n          buttonImage: '@/assets/octopus_button_2.png',\n        },\n        {\n          id: 3,\n          name: '펀드 문어',\n          image: '@/assets/octopus_button_3.png',\n          buttonImage: '@/assets/octopus_button_3.png',\n        },\n        {\n          id: 4,\n          name: '채권 문어',\n          image: '@/assets/octopus_button_4.png',\n          buttonImage: '@/assets/octopus_button_4.png',\n        },\n      ],\n      investments: [\n        { id: 1, name: '예적금', icon: '@/assets/icon_savings.png' },\n        { id: 2, name: '주식', icon: '@/assets/icon_stock.png' },\n        { id: 3, name: '펀드', icon: '@/assets/icon_fund.png' },\n        { id: 4, name: '채권', icon: '@/assets/icon_bond.png' },\n      ],\n    };\n  },\n  computed: {\n    formattedExpenseAmount() {\n      return this.expenseAmount.toLocaleString(); // 숫자를 100,000 형식으로 변환\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* 전체 페이지 스크롤 숨기기 */\n.homepage {\n  background-color: var(--p70);\n  overflow-y: hidden;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  height: 100vh;\n}\n\n/* 헤더 */\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 22px;\n  height: 65px;\n  background-color: var(--p70);\n  position: fixed;\n  top: 44px;\n  left: 0;\n  width: 100%;\n  z-index: 1000;\n  box-sizing: border-box;\n}\n\n.mypage-icon {\n  width: 24px;\n  height: 24px;\n}\n\n/* 문어 레벨 섹션 */\n.level-section {\n  margin: 0 22px;\n  margin-top: 91px;\n}\n\n.level-text {\n  color: var(--gray-20);\n  font-size: 24px;\n  font-weight: 500;\n  line-height: 100%;\n  margin: 0 0 10px 0;\n}\n\n.level-description {\n  color: var(--gr50);\n  font-size: 14px;\n  font-weight: 400;\n  line-height: 100%;\n  margin: 10px 0 0 0;\n  text-decoration: none;\n  cursor: pointer;\n}\n\n.octopus-image {\n  width: 220px;\n  height: 220px;\n  margin: 20px auto;\n  object-fit: contain;\n}\n\n/* 투자 상품 선택 버튼 */\n.investment-buttons {\n  display: flex;\n  justify-content: space-between;\n  padding: 0 14px;\n  margin-top: 20px;\n}\n\n.investment-button {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  background-color: transparent;\n  border: none;\n  cursor: pointer;\n}\n\n.investment-button img {\n  width: 50px;\n  height: 50px;\n  margin-bottom: 5px;\n}\n\n.investment-button p {\n  font-size: 12px;\n  color: var(--gr50);\n  text-align: center;\n}\n\n/* 지출 금액 섹션 */\n.expense-section {\n  position: fixed;\n  bottom: 86px;\n  left: 0;\n  width: 100%;\n  padding: 0;\n}\n\n.expense-container {\n  background-color: var(--p10);\n  border-radius: 20px 20px 0 0;\n  padding: 20px 0;\n  flex-direction: column;\n  align-items: center;\n}\n\n/* 지출 금액 네모 박스 */\n.expense-box {\n  width: 335px;\n  height: 64px;\n  flex-shrink: 0;\n  border-radius: 12px;\n  border: 0.816px solid rgba(255, 255, 255, 0.1);\n  background: rgba(255, 255, 255, 0.2);\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  padding: 0 24px;\n  box-sizing: border-box;\n  margin: 0 20px;\n}\n\n/* 지출 금액 및 금액 표시 */\n.subtitle {\n  color: var(--gr100);\n  font-size: 16px;\n  font-style: normal;\n  font-weight: 400;\n  line-height: 100%;\n}\n\n.amount {\n  color: var(--gr100);\n  text-align: right;\n  font-size: 24px;\n  font-weight: 400;\n  line-height: 100%;\n}\n\n/* 하단 네비게이션 바 */\n.tabbar {\n  display: flex;\n  justify-content: space-between;\n  position: fixed;\n  bottom: 0;\n  left: 0;\n  width: 100%;\n  background-color: white;\n  padding: 10px 0;\n}\n\n.tabbar-item {\n  flex: 1;\n  text-align: center;\n  color: var(--gr50);\n  text-decoration: none;\n  font-size: 14px;\n}\n</style>\n"],"mappings":"AAgEA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,IAAI;MAC